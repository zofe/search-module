{
    "name": "zofe/search-module",
    "description": "search module for laravel rapyd-admin application",
    "license": "mit",
    "type": "rapyd-module",
    "authors": [
        {
            "name": "Felice",
            "email": "felice.ostuni@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/config": "^11.0|^12.0",
        "illuminate/contracts": "^11.0|^12.0",
        "laravel/scout": "^10.0",
        "meilisearch/meilisearch-php": "^1.0",
        "http-interop/http-factory-guzzle": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "zofe/rapyd-module-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "App\\Modules\\Search\\SearchServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
